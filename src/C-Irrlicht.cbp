			<Option compilerVar="CC" />
		</Unit>
		<Unit filename="gui.c">
			<Option compilerVar="CC" />
		</Unit>
		<Unit filename="io.c">
			<Option compilerVar="CC" />
		</Unit>
		<Unit filename="irrlicht.c">
			<Option compilerVar="CC" />
		</Unit>
		<Unit filename="scene.c">
			<Option compilerVar="CC" />
		</Unit>
		<Unit filename="video.c">
			<Option compilerVar="CC" />
		</Unit>
		<Extensions>
